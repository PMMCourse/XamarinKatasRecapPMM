
<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XamarinFormsKatas.Katas_UI.Kata_s.KataS">
    <ContentPage.Content>
       <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinition>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinition>
            <BoxView Grid.Row="0" Grid.Column="0" BackgroundColor="Yellow"/>
            <BoxView Grid.Row="0" Grid.Column="1" BackgroundColor="Blue"/>
            <BoxView Grid.Row="1" Grid.Column="0" BackgroundColor="Red"/>
            <BoxView Grid.Row="1" Grid.Column="1" BackgroundColor="Orange"/>
        </Grid>
    </ContentPage.Content>
</ContentPage>